{
    "manifest_version": 3,
    "name": "RobuxHider",
    "version": "1.0.0",
    "permissions": ["storage","activeTab","scripting"],
    "host_permissions": [
        "https://www.roblox.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {"default_title": "Click Me To Switch On/Off"}
}